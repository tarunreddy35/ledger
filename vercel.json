{
  "version": 2,
  "functions": {
    "smartledger-backend/src/index.js": {
      "runtime": "@vercel/node@3"
    }
  },
  "routes": [
    {
      "src": "/trpc/(.*)",
      "dest": "smartledger-backend/src/index.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "smartledger-backend/src/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "frontend/dist/$1"
    }
  ],
  "buildCommand": "cd frontend && npm install && npm run build",
  "outputDirectory": "frontend/dist",
  "installCommand": "cd smartledger-backend && npm install",
  "env": {
    "NODE_ENV": "production"
  }
}
